<p id="notice"><%= notice %></p>

<h1>Meetings</h1>


<%= month_calendar events: @meetings do |date, meetings| %>
  <%= format_date(date) %>

  <% meetings.each do |meeting| %>
    <div>
      <%= link_to meeting.name, meeting_path(meeting.id) %> (<%= format_hour(meeting.start_time) %>)
    </div>
  <% end %>
<% end %>
